      }
    },
    {
      "cell_type": "code",
      "source": [
        "def print_sqr(L1):\n",
        "  L2=[]\n",
        "  for i in L1:\n",
        "    L2.append(i**2)\n",
        "  print(L2, end=\" \")"
      ],
      "metadata": {
