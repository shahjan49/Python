      "source": [
        "Take any word as input from the user and count the number of vowels in the word."
      ],
      "metadata": {
        "id": "3KsJoRr7W98B"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "word=input(\"enter any word: \")\n",
        "count=0\n",
        "i=0\n",
        "vowels=[\"a\",\"e\",\"i\",\"o\",\"u\"]\n",
        "for i in range(len(word)):\n",
        "  if word[i] in vowels:\n",
        "    count+=1\n",
        "print(\"The no. of vowels in this word are\",count)"
      ],
